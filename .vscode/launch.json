  "version": "0.2.0",
  "configurations": [
    {
      "name": "nestjs",
      "request": "launch",
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "run",
        "dev"
      ],
      "type": "node",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "meta data",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ],
      "program": "${workspaceFolder}/decorator/params.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}